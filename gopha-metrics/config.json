{
  "size": "1920x1080",
  "rate": 60,

  "reference": { "pixel_format": "yuv444p" },
  "converted":  { "pixel_format": "yuv420p" },

  "metric_processing": {
    "pixel_format": "yuv444p",
    "normalize_range": "tv"
  },

  "color": {
    "use_zscale": true,
    "range": "tv",
    "matrix": "bt709",
    "primaries": "bt709",
    "transfer": "bt709"
  },

  "rewrap": {
    "enabled": true,
    "codec": "ffv1",
    "ffv1_level": 3,
    "g": 1,
    "slicecrc": 1
  },

  "cycle_length": 60,

  "metrics": {
    "vmaf": true,
    "ssim": true,
    "psnr": true,
    "temporal_ssim": true,
    "frame_mse": true,
    "cycle_analysis": true
  },

  "error_video": {
    "difference": true,
    "amplified_difference": true,
    "heatmap": true
  },
  
  "output_directory": "results",
  "vmaf_model_path": null
}